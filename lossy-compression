        var s = document.getElementsByTagName('script')[0];

        
        if (document.querySelector('.twitter-tweet') || document.querySelector("a[href*='twitter.com'][href*='status']")) {
            var twitter = document.createElement('script');
            twitter.src = "https://platform.twitter.com/widgets.js";
            s.parentNode.insertBefore(twitter, s);
        }

        // Empty image alt text
        setTimeout(function() {
            document.querySelectorAll('img:not([alt])').forEach(function(element) {
                element.setAttribute('alt', element.src);
            });
        }, 500);

        // Core Key/Value Tracking
        if (!window.zdcoreSignalBuffer){ window.zdcoreSignalBuffer = [];}
        window.zdcoreSignalBuffer.push(["Generic KV Tracking", {namespace: "ZDRPP", key: "template", value: window.template || null}]);
    })();
</script>
